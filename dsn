#! /usr/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x92395b0 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x9236078_0 .var "clk", 0 0;
v0x92360f8_0 .var "reset", 0 0;
v0x9236168_0 .net "value", 7 0, v0x9235fb8_0; 1 drivers
S_0x9215fd8 .scope module, "c1" "counter" 2 21, 3 1, S_0x92395b0;
 .timescale 0 0;
P_0x9239ee4 .param/l "WIDTH" 3 3, +C4<01000>;
v0x92397d0_0 .net "clk", 0 0, v0x9236078_0; 1 drivers
v0x9235fb8_0 .var "out", 7 0;
v0x9236018_0 .net "reset", 0 0, v0x92360f8_0; 1 drivers
E_0x9239a30 .event edge, v0x9236018_0;
E_0x92399a8 .event posedge, v0x92397d0_0;
    .scope S_0x9215fd8;
T_0 ;
    %wait E_0x92399a8;
    %load/v 8, v0x9235fb8_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %ix/load 0, 8, 0;
    %assign/v0 v0x9235fb8_0, 0, 8;
    %jmp T_0;
    .thread T_0;
    .scope S_0x9215fd8;
T_1 ;
    %wait E_0x9239a30;
    %load/v 8, v0x9236018_0, 1;
    %jmp/0xz  T_1.0, 8;
    %cassign/v v0x9235fb8_0, 0, 8;
    %jmp T_1.1;
T_1.0 ;
    %deassign v0x9235fb8_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x92395b0;
T_2 ;
    %set/v v0x92360f8_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x92395b0;
T_3 ;
    %vpi_call 2 6 "$dumpfile", "test.vcd";
    %vpi_call 2 7 "$dumpvars", 1'sb0, S_0x92395b0;
    %delay 17, 0;
    %set/v v0x92360f8_0, 1, 1;
    %delay 11, 0;
    %set/v v0x92360f8_0, 0, 1;
    %delay 29, 0;
    %set/v v0x92360f8_0, 1, 1;
    %delay 5, 0;
    %set/v v0x92360f8_0, 0, 1;
    %delay 513, 0;
    %vpi_call 2 13 "$finish";
    %end;
    .thread T_3;
    .scope S_0x92395b0;
T_4 ;
    %set/v v0x9236078_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x92395b0;
T_5 ;
    %delay 1, 0;
    %load/v 8, v0x9236078_0, 1;
    %inv 8, 1;
    %set/v v0x9236078_0, 8, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x92395b0;
T_6 ;
    %vpi_call 2 24 "$monitor", "At time %t, value = %h (%0d)", $time, v0x9236168_0, v0x9236168_0;
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
